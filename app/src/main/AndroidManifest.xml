<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.flowerroad.requestviewmodel">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.requestVM.demo.MyApp"
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/netowrk"
        android:theme="@style/Theme.RequestViewModel">
        <activity android:name="com.requestVM.demo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.requestVM.demo.viewmodel.SecondActivity"/>

    </application>

</manifest>